<section>
  <h2>Login to <%= @contest.name %></h2>
  <form action="/<%= @contest.name %>/login" method="post">
    <table>
      <tr>
        <td><label for="teamid">Team ID:</label></td>
        <td><input type="text" id="teamid" name="teamid" value=""></td>
      </tr>
      <tr>
        <td><label for="passwd">Password:</label></td>
        <td><input type="text" id="passwd" name="passwd" value=""></td>
      </tr>
      <tr>
        <td></td>
        <td><input type="submit" value="Login"></td>
      </tr>
    </table>
  </form>
  <h2>Team List</h2>
  <p><a href="/<%= @contest.name %>/teamlist">List of Teams</a></p>
  <h2>Register to <%= @contest.name %></h2>
  <p><a href="/<%= @contest.name %>/register">Register</a></p>
</section>
